    "APIHandler": {
      "Type": "AWS::Serverless::Function",
      "Properties": {
        "Runtime": "python3.6",
        "Handler": "app.app",
        "CodeUri": { 
          "Bucket" : { "Ref": "DeploymentBucket" },
          "Key" : { "Ref": "LambdaPackagePath" }
        },
        "Tags": {
          "aws-chalice": "version=1.7.0:stage=dev:app=Detector-API"
        },
        "Timeout": 60,
        "MemorySize": 128,
        "Role": {
          "Fn::GetAtt": [
            "CorrosionDetectionLambdaRole",
            "Arn"
          ]
        }
      }
    },
    "InferenceHandler": {
      "Type": "AWS::Serverless::Function",
      "Properties": {
        "Runtime": "python2.7",
        "Handler": "app.app",
        "CodeUri": { 
          "Bucket" : { "Ref": "DeploymentBucket" },
          "Key" : { "Ref": "InferenceLambdaPackagePath" }
        },
        "Tags": {
          "aws-chalice": "version=1.7.0:stage=dev:app=Detector-API"
        },
        "Timeout": 60,
        "MemorySize": 128,
        "Environment": {
          "Variables": {
            "CORROSION_IMAGE_S3_BUCKET": { "Ref": "ImagesBucketName" },
            "CORROSION_TRAINING_JOB_STATE_MACHINE_ARN": { "Ref": "TrainingArn" },
            "CORROSION_ENDPOINT_JOB_STATE_MACHINE_ARN": { "Ref": "EndpointArn" },
            "MODEL_ENDPOINT_JOB_STATE_MACHINE_ARN": { "Ref": "TrainingEndpointArn" },
            "COGNITO_USER_POOL_ARN": "",
            "COGNITO_USER_POOL_NAME": ""
          }
        },
        "Role": {
          "Fn::GetAtt": [
            "CorrosionDetectionLambdaRole",
            "Arn"
          ]
        }
      }
    },
